package sec1;

public class Type2 {
	public static void main(String[] args) {
		//reference type(참조형) : 다수의 데이터를 저장하기 위한 기억장소로서 null값이 허용됨
		int[] arr1 = {10,20,30};
		int[] arr2 = {10,20,30};
		int[] arr3 = null;
		//int a = null; primitive type은 null일 수 없음
		arr3[0] = 20;
		System.out.println(arr3[1]);
		System.out.println(arr3[0]);
	}
}