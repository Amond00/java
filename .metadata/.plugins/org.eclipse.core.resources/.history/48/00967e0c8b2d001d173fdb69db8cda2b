package sec1;

public class Oper4 {
	public static void main(String[] args) {
		int a=200, b=100, c=20, d=10;
		//비교연산자 (==,!=,>,>=,<,<=) 초과=후 이상=이후 미만=전 이하=이전 미ture or false 출력 
		System.out.println("a>b => "+(a==b));
		System.out.println("a!=b => "+(a!=b);
		System.out.println("a>b => "+(a>b));
		System.out.println("a>=b => "+(a>=b));
		System.out.println("a<b => "+(a<b));
		System.out.println("a<=b => "+(a<=b));
	}
}
