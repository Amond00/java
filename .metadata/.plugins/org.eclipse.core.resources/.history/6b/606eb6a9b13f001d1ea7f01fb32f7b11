package sec1;
//중첩 클래스
public class OuterClass {//외부 클래스
	int field1;
	public OuterClass() { }
	void method1() { }
	
	
	//내부(멤버)클래스
	class Inner{
		int field1;
		public Inner() { }
		public void method1() { }
			
	}
	//내부(멤버)클래스
	static class Inner2 {
		int field1;
		public Inner2() { }
		public void method1() { }
	}
}
