package sec2;

public class Throw {
	public static void main(String[] args) {
		String name = null;
		nullPassing(name);
		
		int[] arr = new int[3];
		arrayPssing(arr);
	}	
	private static void arrayPssing(int[] arr) throws NullPointerException, ArrayIndexOutOfBoundsException, Exception{
		for(int i=0;i<5;i++){
			arr[i] = i*10;
		}
	}
	static void nullPassing(String data) throws NullPointerException{
		System.out.println(data);
		
	}
}