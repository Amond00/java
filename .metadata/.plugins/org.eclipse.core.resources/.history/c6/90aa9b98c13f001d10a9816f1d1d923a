package sec1;

public class Exception4 {
	static class Animal{}
	static class Dog extends Animal{}
	static class Cat extends Animal{}
	public static void main(String[] args) {
		Dog dog = new Dog();
		changeDong(dog);
		
		Cat cat = new Cat();
		changeDong(cat);
		
	}
	public static void changeDong(Animal animal){
		try{
		Dog dog = (Dog) animal;
		}
	}	
}