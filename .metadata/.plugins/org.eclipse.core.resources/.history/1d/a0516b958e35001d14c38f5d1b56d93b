package sec1;

public class Type3 {
	public static void main(String[] args) {
		String name = "han"; //참조형이지만 완벽한 참조형은 아님
		char[] irum = {'h','a','n'};
		String nick = "han";
		String lans = new String("han");
		System.out.println(name);
		System.out.println(irum);
		//System.out.println(name==irum); - 서로 비교대상이 될 수 없다.
		System.out.println(name==nick);
		System.out.println(name!=nick);
		System.out.println();
		System.out.println(name==lans); //서로 같지 않음
		System.out.println(name!=lans);
		System.out.println();
		System.out.println(name.equals(lans)); //서로 다른 타입 비교할때 사용
		System.out.println(!name.equals(lans));
		
	}
}