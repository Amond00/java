package sec1;

public class ChildEx1 {
	public static void main(String[] args) {
		Child c1 = new Child();
		c1.field1 = 10;
		c1.field2 = "kim";
		c1.field3 = 20;
		
		Parent c2 = new Parent();
		c2.field1 = 100;
		c2.field2 = "lee";
		//c2.field3 = 200;
		
		Parent c3;
		c3 = new Parent();
		c3.method1();
		System.out.println(c2.method2());
		
		c3 = new Child();
		System.out.println(c3.method2());
		c3.method2();
		//c3.method3();
		//c3.method4();
	}
}