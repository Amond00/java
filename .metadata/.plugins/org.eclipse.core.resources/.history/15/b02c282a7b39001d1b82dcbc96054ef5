package sec2;
import java.util.Scanner;
public class Board {
	Scanner sc = new Scanner(System.in);
	int wrnum;
	int wrtitle;
	int wrcontent;
	int wrhost;
	int wrdate;
	public Board(int wrnum, int wrtitle, int wrcontent, int wrhost, int wrdate) {
		this.wrnum = wrnum;
		this.wrtitle = wrtitle;
		this.wrcontent = wrcontent;
		this.wrhost = wrhost;
		this.wrdate = wrdate;
	}
	public int getWrnum() {
		return wrnum;
	}
	public void setWrnum(int wrnum) {
		this.wrnum = wrnum;
	}
	public int getWrtitle() {
		return wrtitle;
	}
	public void setWrtitle(int wrtitle) {
		this.wrtitle = wrtitle;
	}
	public int getWrcontent() {
		return wrcontent;
	}
	public void setWrcontent(int wrcontent) {
		this.wrcontent = wrcontent;
	}
	public int getWrhost() {
		return wrhost;
	}
	public void setWrhost(int wrhost) {
		this.wrhost = wrhost;
	}
	public int getWrdate() {
		return wrdate;
	}
	public void setWrdate(int wrdate) {
		this.wrdate = wrdate;
	}
	scanner.close();
}	
